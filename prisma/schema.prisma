generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model Link {
  id        String   @id @default(cuid())
  createdAt DateTime @default(now())
  slug      String   @unique
  url       String   @db.LongText

  @@index([slug])
}
